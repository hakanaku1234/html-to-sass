{
    "name": "htmltosass",
    "displayName": "html to sass",
    "description": "Turn your html or jsx to sass format",
    "version": "1.0.0",
    "publisher": "nosecoolie",
    "engines": {
        "vscode": "^1.28.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.sayHello"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.sayHello",
                "title": "html to sass"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "dependencies": {
        "copy-paste": "1.3.0",
        "clipboardy": "1.2.3"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "eslint": "^4.11.0",
        "copy-paste": "1.3.0",
        "clipboardy": "1.2.3",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}
